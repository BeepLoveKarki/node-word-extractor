{
  "name": "word-extractor",
  "version": "0.1.4",
  "description": "Node.js package to read Word .doc files",
  "main": "src/word.js",
  "scripts": {
    "test": "mocha --sort --recursive ./test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/morungos/node-word-extractor.git"
  },
  "keywords": [
    "word"
  ],
  "author": "Stuart Watt <stuart@morungos.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/morungos/node-word-extractor/issues"
  },
  "homepage": "https://github.com/morungos/node-word-extractor",
  "devDependencies": {
    "chai": "^1.10.0",
    "chai-as-promised": "^4.1.1",
    "eslint": "^5.8.0",
    "mocha": "^2.2.5"
  },
  "dependencies": {
    "async": "^1.5.2",
    "bluebird": "^2.6.0",
    "event-stream": "^3.3.2",
    "underscore": "^1.8.3",
    "xmldom": "^0.1.27",
    "xpath": "0.0.24",
    "yauzl": "^2.8.0"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "browser": true,
      "mocha": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 8
    },
    "rules": {
      "semi": [
        2,
        "always"
      ],
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "all",
          "ignoreRestSiblings": false,
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
